}

model Property {
  id          String    @id @default(cuid())
  no          String    @default("")
  owner       Customer? @relation("Property_owner", fields: [ownerId], references: [id])
  ownerId     String?   @map("owner")
  project     Project?  @relation("Property_project", fields: [projectId], references: [id])
